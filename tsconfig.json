{
  "compilerOptions": {                    
    "target": "esnext", 
    "module": "esnext", 
    "lib": [
      "dom",
      "dom.iterable"
    ],
    "jsx": "preserve",
    "downlevelIteration": true,
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true ,
    "allowJs": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
